<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<preConditions>
		<dbms type="oracle" />
	</preConditions>

	<!-- Create "liquibase-start" tag in the database. This will allow the developer 
		to roll back *ALL* database changes applied with Liquibase. -->
	<changeSet id="0.0.0" author="iAxis-changelog-master">
		<tagDatabase tag="liquibase-start" />
	</changeSet>

		<!-- Change logs for each version -->
	<include relativeToChangelogFile="true" file="changelog-0.0.1/changelog-0.0.1.xml" />

</databaseChangeLog>
